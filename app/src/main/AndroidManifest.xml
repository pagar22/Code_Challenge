<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.code_challenge">

    <application
        android:allowBackup="true"
        android:icon="@drawable/my_mommy_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/my_mommy_icon"
        android:resizeableActivity="false"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">

        </activity>

        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NicheTasks"/>
        <activity android:name=".NicheTasksList"/>
        <activity android:name=".NicheTasksAdd"/>
        <activity android:name=".PointsTally"/>
        <activity android:name=".Registration"
            android:noHistory="true"/>
        <activity android:name=".QuickStart"/>
        <activity android:name=".Profile"/>
        <activity android:name=".MMCWinner"
            android:noHistory="true"/>

        <receiver android:name=".NotificationBroadcastBuilder"/>

    </application>
</manifest>